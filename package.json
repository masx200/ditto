{
    "type": "commonjs",
    "version": "3.5.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com/"
    },
    "name": "@masx200/markdown-reader",
    "main": "./src/export.js",
    "types": "./src/export.d.ts",
    "scripts": {
        "build": "npm run prod &&npm run copyfiles",
        "start": "npm run linkfiles &&npm run dev",
        "linkfiles": "node utils/linkfiles.js --type=link",
        "copyfiles": "node utils/linkfiles.js --type=copy",
        "dev": "webpack-react-vue-spa-awesome-config start --port=11111",
        "prod": " webpack-react-vue-spa-awesome-config build   ",
        "format": "prettier --write *.html *.js *.css *.md *.json *.vue */*.ts *.ts */*/*.ts */*.html */**/*.js */*.css */*.md */*.json */*.vue */*.js"
    },
    "devDependencies": {
        "@highlightjs/cdn-assets": "^11.0.1",
        "@masx200/webpack-react-vue-spa-awesome-config": "3.3.0",
        "@types/highlight.js": "^10.1.0",
        "@types/marked": "^2.0.3",
        "@vue/composition-api": "^1.0.0-rc.11",
        "clipboard": "^2.0.8",
        "highlight.js": "^11.0.1",
        "marked": "^2.0.7",
        "prettier": "2.3.1",
        "terser": "^5.7.0",
        "terser-webpack-plugin": "^5.1.3",
        "tslib": "^2.2.0",
        "typescript": "^4.3.2",
        "vue": "^2.6.14",
        "webpack": "^5.38.1"
    },
    "description": "Lightweight Markdown Documentation System ",
    "directories": {
        "doc": "docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/masx200/markdown-reader.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/masx200/markdown-reader/issues"
    },
    "homepage": "https://github.com/masx200/markdown-reader#readme",
    "dependencies": {
        "@masx200/mini-cli-args-parser": "1.0.4",
        "fs-extra": "^10.0.0"
    }
}
